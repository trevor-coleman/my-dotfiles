  "name": "Project Template",
  "packageManager": "yarn@3.0.2",
  "scripts": {
    "clean": "run-p clean:build clean:dist",
    "clean:build": "rimraf build && echo \"Cleaned ./build\"",
    "clean:dist": "rimraf dist  && echo \"Cleaved ./dist\"",
    "build": "run-s clean build:bundle build:storybook clean:build",
    "build:bundle": "run-p --print-label  build:types build:vite",
    "build:components": "run-s clean build:types build:bundle",
    "build:types": "run-s build:dts",
    "build:tsc": "tsc -p tsconfig.build.json",
    "build:dts": "dts-bundle-generator --project tsconfig.build.json --config dts.config.js ",
    "build:rollup:DEPRECATED": "rollup -c rollup.config.js",
    "build:storybook": "build-storybook -o dist/storybook",
    "build:storybook:watch": "build-storybook -o dist/storybook -w",
    "build:vite": "vite build -c vite.config.production.js",
    "build:esbuild": "node ./esbuild.config.js",
    "build:try": "run-s --print-label build:bundle prerelease",
    "commit": "cz",
    "commit:whoops": "git commit --amend --no-edit",
    "commit:all": "git add -A && git commit --no-verify -m",
    "new:component": "hygen component new",
    "dev": "vite",
    "serve": "vite preview",
    "serve:storybook": "vite preview -c ./vite.config.storybook.ts",
    "storybook": "start-storybook -p 6006",
    "test": "echo no tests",
    "lint": " run-s lint:eslint lint:prettier",
    "lint:eslint": "eslint --fix \"src/**/*.{ts,tsx}\" --no-error-on-unmatched-pattern",
    "lint:prettier": "prettier --config .prettierrc -w ./src/**/*.ts ./src/**/*.tsx ./*.html",
    "lint:types": "tsc -p tsconfig.lint.json",
    "check": "run-p lint test ",
    "push:version": "git push --follow-tags origin master",
    "release": "standard-version",
    "prerelease": "standard-version --prerelease",
    "iterate": "run-s build:bundle prerelease && yarn npm publish",
    "release:push": "run-s --print-label check build release push:version  && yarn npm publish"
  },
  "version": "0.2.33-1",
